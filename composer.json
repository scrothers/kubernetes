{
    "name": "scrothers/kubernetes",
    "description": "A Kubernetes API SDK library.",
    "type": "library",
    "prefer-stable": true,
    "license": "MIT",
    "minimum-stability": "stable",
    "support": {
        "issues": "https://github.com/scrothers/kubernetes/issues",
        "rss": "https://github.com/scrothers/kubernetes/releases.atom",
        "source": "https://github.com/scrothers/kubernetes",
        "wiki": "https://github.com/scrothers/kubernetes/wiki"
    },
    "autoload": {
        "psr-4": {
            "Crothers\\Kubernetes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Crothers\\Kubernetes\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Steven Crothers",
            "email": "steven@scrothers.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Crothers\\Kubernetes\\LaravelServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "^7.5",
        "guzzlehttp/psr7": "^2.5",
        "illuminate/support": "^10.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "phpunit/php-invoker": "^4.0"
    }
}
